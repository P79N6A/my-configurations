#!/Library/Frameworks/Python.framework/Versions/3.4/bin/python3
'''
Replace oldstr(the first argument) with newstr(the second argument) in file names under current directory
'''

import os, sys
oldstr = sys.argv[1]
newstr = sys.argv[2]

for oldname in os.listdir('.'):
    newname = oldname.replace(oldstr, newstr)
    print(oldname+"  ------>  "+newname)
    os.rename(oldname, newname)

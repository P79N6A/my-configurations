[color]
    ui = true
[alias]
    st = status
    adda = add --all .
    cm = commit -m
    ac = commit -am 
    logo = log --oneline
    br = branch
    co = checkout
    lf = log --pretty=format:\"%h -- %an -- %ar -- %s\"
    lbr = log --graph --all --decorate
    lb = log --graph --all --decorate
    dbr = branch -d
    sync = fetch origin master:master
[push]
    default = matching
[include]
    path = $HOME/.gitconfig_local
